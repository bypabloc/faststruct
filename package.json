{
    "name": "faststruct",
    "displayName": "FastStruct",
    "description": "Quickly create and manage code structures",
    "version": "0.0.1",
    "type": "commonjs",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": ["Other"],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./src/main.js",
    "contributes": {
        "commands": [
            {
                "command": "faststruct.createStructure",
                "title": "FastStruct: Create Structure"
            }
        ],
        "configuration": {
            "title": "FastStruct",
            "properties": {
                "faststruct.useTypeScript": {
                    "type": "boolean",
                    "default": false,
                    "description": "Use TypeScript structure"
                },
                "faststruct.includeTests": {
                    "type": "boolean",
                    "default": true,
                    "description": "Include test directory"
                }
            }
        }
    },
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "@vscode/test-electron": "^2.3.8",
        "jest": "^29.0.0",
        "eslint": "^8.0.0"
    }
}
